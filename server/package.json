{
  "name": "gogs",
  "version": "0.0.0",
  "description": "Gog Server",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "console": "ts-node-dev --experimental-repl-await --respawn --transpileOnly console.ts",
    "start": "ts-node-dev --respawn --transpileOnly src/index.ts",
    "tsc": "tsc",
    "build": "node build.js",
    "db:seed": "yarn run ts-node src/db/seeds.ts"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "testPathIgnorePatterns": [
      "/__tests__/helpers/",
      "/dist/",
      "/__tests__/factories/"
    ]
  },
  "author": "WDD",
  "license": "MIT",
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "ts-node -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@shared": "dist/shared",
    "@server": "dist/Server"
  },
  "dependencies": {
    "@types/faker": "^4.1.12",
    "@types/js-yaml": "^3.12.5",
    "@types/knex-db-manager": "^0.6.1",
    "bcrypt": "^5.0.0",
    "command-line-args": "^5.1.1",
    "cookie-parser": "^1.4.5",
    "csv": "^5.3.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "faker": "^4.1.0",
    "helmet": "^3.23.1",
    "http-status-codes": "^1.4.0",
    "js-yaml": "^3.14.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.4.0",
    "knex-db-manager": "^0.6.1",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "objection": "^2.2.0",
    "pg": "^8.2.1",
    "pg-escape": "^0.2.0",
    "randomstring": "^1.1.5",
    "repl.history": "^0.1.4",
    "ts-node-dev": "^1.0.0-pre.49",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-typescript": "^7.10.1",
    "@types/bcrypt": "^3.0.0",
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.6",
    "@types/find": "^0.2.1",
    "@types/helmet": "0.0.47",
    "@types/http-status-codes": "^1.2.0",
    "@types/jasmine": "^3.5.10",
    "@types/jest": "^26.0.3",
    "@types/jsonfile": "^6.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/knex": "^0.16.1",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.13",
    "@types/randomstring": "^1.1.6",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/eslint-plugin-tslint": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "babel-jest": "^26.1.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsdoc": "^27.1.1",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "find": "^0.3.0",
    "fs-extra": "^9.0.1",
    "jasmine": "^3.5.0",
    "jest": "^26.1.0",
    "jsonfile": "^6.0.1",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tslint-to-eslint-config": "^1.0.0",
    "typescript": "^3.9.5"
  }
}
